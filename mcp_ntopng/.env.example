# Required ntopng ClickHouse connection settings
NTOPNG_HOST=test.ntoplink.com
NTOPNG_DBUSER=default
NTOPNG_DBPASSWORD=

# Optional ntopng ClickHouse connection settings
NTOPNG_DBPORT=9000          # Default: 9440 if secure=True, 9000 if secure=False
NTOPNG_SECURE=false         # Default: false (values: true/false)
NTOPNG_VERIFY=true          # Default: true (values: true/false)
NTOPNG_CONNECT_TIMEOUT=30   # Default: 30 seconds
NTOPNG_SEND_RECEIVE_TIMEOUT=300  # Default: 300 seconds

# Application settings
SELECT_QUERY_TIMEOUT_SECS=30  # Default: 30 seconds
